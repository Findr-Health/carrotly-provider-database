# ========================================
# AI AGENT CONFIGURATION
# Portable configuration for any backend
# ========================================

# Backend API Configuration
API_BASE_URL=http://localhost:3000/api
API_ADMIN_EMAIL=admin@carrotly.com
API_ADMIN_PASSWORD=admin123

# Google Maps API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
GOOGLE_MAPS_PLACES_API_KEY=your_places_api_key_here

# OpenAI API (for data extraction)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-turbo-preview

# Agent Settings
AGENT_MAX_PROFILES_PER_RUN=25
AGENT_MIN_CONFIDENCE_SCORE=70
AGENT_DUPLICATE_THRESHOLD_EXACT=100
AGENT_DUPLICATE_THRESHOLD_HIGH=85
AGENT_DUPLICATE_THRESHOLD_MEDIUM=70

# Scraping Settings
SCRAPER_USER_AGENT=Mozilla/5.0 (compatible; CarrotlyBot/1.0; +https://carrotly.com/bot)
SCRAPER_TIMEOUT_SECONDS=30
SCRAPER_MAX_RETRIES=3
SCRAPER_RESPECT_ROBOTS_TXT=true
SCRAPER_RATE_LIMIT_SECONDS=2

# Data Quality Settings
MIN_REQUIRED_FIELDS=practice_name,address,city,state,zip,phone
MIN_SERVICES_FOR_HIGH_CONFIDENCE=3
MIN_PHOTOS_FOR_HIGH_CONFIDENCE=1

# Export Settings
EXPORT_DIRECTORY=./exports
EXPORT_FORMAT=json

# Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/agent.log
